#: common.c:220
#, c-format
msgid "Usage: %s [options] [file ...] [-n infile outfile ...]\n"
msgstr ""
"Uso: %s [parámetros] [archivo ...] [-n archivo_de_entrada "
"archivo_de_salida ...]\n"

#: common.c:221
#, c-format
msgid " -ascii                convert only line breaks (default)\n"
msgstr ""
" -ascii                solamente convierte los saltos de línea (por "
"defecto)\n"

